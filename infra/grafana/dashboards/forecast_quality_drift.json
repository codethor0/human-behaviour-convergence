  "templating": {
    "list": [
      {
        "name": "region",
        "type": "query",
        "label": "Region",
        "datasource": {
          "type": "prometheus",
          "uid": "PBFA97CFB590B2093"
        },
        "query": "label_values(behavior_index, region)",
        "multi": false,
        "includeAll": false,
        "current": {
          "selected": false,
          "text": "city_nyc",
          "value": "city_nyc"
        },
        "refresh": 1,
        "sort": 1
      }
    ]
