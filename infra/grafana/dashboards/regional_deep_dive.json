{
  "title": "Regional Deep Dive",
  "uid": "regional-deep-dive",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-30d",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "region",
        "type": "query",
        "label": "Region",
        "datasource": {
          "type": "prometheus",
          "uid": "PBFA97CFB590B2093"
        },
        "query": "label_values(behavior_index, region)",
        "multi": false,
        "includeAll": false,
        "current": {
          "selected": false,
          "text": "city_nyc",
          "value": "city_nyc"
        },
        "refresh": 1,
        "sort": 1
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "Behavior Index - $region",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "behavior_index{region=\"$region\"}",
          "legendFormat": "Behavior Index",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "Behavior Index",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            },
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": {
              "type": "linear"
            },
            "showPoints": "never",
            "spanNulls": false
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 0.6,
                "color": "yellow"
              },
              {
                "value": 0.8,
                "color": "red"
              }
            ]
          },
          "unit": "short",
          "min": 0,
          "max": 1
        }
      },
      "options": {
        "legend": {
          "calcs": ["last", "mean", "max"],
          "displayMode": "table",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      }
    },
    {
      "id": 2,
      "title": "Parent Sub-Indices - $region",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "parent_subindex_value{region=\"$region\"}",
          "legendFormat": "{{parent}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "Sub-Index Value",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            },
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": {
              "type": "linear"
            },
            "showPoints": "never",
            "spanNulls": false
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 0.5,
                "color": "yellow"
              },
              {
                "value": 0.7,
                "color": "red"
              }
            ]
          },
          "unit": "short",
          "min": 0,
          "max": 1
        }
      },
      "options": {
        "legend": {
          "calcs": ["last", "mean"],
          "displayMode": "table",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      }
    },
    {
      "id": 3,
      "title": "Current Parent Sub-Index Values - $region",
      "type": "bargauge",
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 16
      },
      "targets": [
        {
          "expr": "parent_subindex_value{region=\"$region\"}",
          "legendFormat": "{{parent}}",
          "refId": "A",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 0.5,
                "color": "yellow"
              },
              {
                "value": 0.7,
                "color": "red"
              }
            ]
          },
          "unit": "short",
          "min": 0,
          "max": 1
        }
      },
      "options": {
        "displayMode": "gradient",
        "orientation": "horizontal",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "showUnfilled": true
      }
    },
    {
      "id": 4,
      "title": "Top Child Sub-Index Contributors - $region",
      "type": "table",
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 12,
        "y": 16
      },
      "targets": [
        {
          "expr": "topk(10, child_subindex_value{region=\"$region\"})",
          "format": "table",
          "instant": true,
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "custom": {
            "align": "auto",
            "cellOptions": {
              "type": "auto"
            },
            "inspect": false
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 0.5,
                "color": "yellow"
              },
              {
                "value": 0.7,
                "color": "red"
              }
            ]
          },
          "unit": "short"
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "parent"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 150
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "child"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 200
              }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "desc": true,
            "displayName": "Value"
          }
        ]
      },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true
            },
            "indexByName": {},
            "renameByName": {
              "Value": "Value",
              "parent": "Parent",
              "child": "Child"
            }
          }
        }
      ]
    },
    {
      "id": 5,
      "title": "Global vs Regional Index Classification - $region",
      "type": "table",
      "gridPos": {
        "h": 6,
        "w": 24,
        "x": 0,
        "y": 22
      },
      "datasource": {
        "type": "grafana",
        "uid": "-- Grafana --"
      },
      "targets": [
        {
          "datasource": {
            "type": "grafana",
            "uid": "-- Grafana --"
          },
          "refId": "A",
          "scenarioId": "csv_content",
          "csvContent": "Index,Type,Description\neconomic_stress,Mixed,Global/national components + regional fuel_stress (varies by state)\nenvironmental_stress,Regional,State/city-specific weather and environmental conditions\nmobility_activity,Global,National TSA passenger data (same for all US regions)\ndigital_attention,Global,Global GDELT tone aggregate (same across regions)\npublic_health_stress,Mixed,May be global if using OWID fallback, or regional if configured\npolitical_stress,Regional,State/city-specific political activity\ncrime_stress,Regional,State/city-specific crime rates\nmisinformation_stress,Regional,State/city-specific information environment\nsocial_cohesion_stress,Regional,State/city-specific social dynamics\nfuel_stress,Regional,State-level EIA gasoline prices (MVP1)\ndrought_stress,Regional,State-level U.S. Drought Monitor (MVP2)\nstorm_severity_stress,Regional,State-level NOAA storm events (MVP3)\nheatwave_stress,Regional,State-level heatwave events (MVP3)\nflood_risk_stress,Regional,State-level flood risk (MVP3)",
          "csvFileName": "global_vs_regional.csv"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "custom": {
            "align": "auto",
            "cellOptions": {
              "type": "auto"
            },
            "inspect": false
          },
          "mappings": [
            {
              "options": {
                "Global": {
                  "color": "blue",
                  "index": 0,
                  "text": "GLOBAL"
                },
                "Regional": {
                  "color": "green",
                  "index": 1,
                  "text": "REGIONAL"
                }
              },
              "type": "value"
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Index"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 200
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Type"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 120
              }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true
      }
    },
    {
      "id": 6,
      "title": "Anomaly status ($region)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 8, "x": 0, "y": 28 },
      "targets": [
        { "expr": "behavior_index_static_anomaly{region=\"$region\"}", "refId": "A", "instant": true },
        { "expr": "behavior_index_zscore_anomaly{region=\"$region\"}", "refId": "B", "instant": true },
        { "expr": "behavior_index_seasonal_anomaly{region=\"$region\"}", "refId": "C", "instant": true },
        { "expr": "behavior_index_residual_anomaly{region=\"$region\"}", "refId": "D", "instant": true },
        { "expr": "hbc_multivariate_md_anomaly{region=\"$region\"}", "refId": "E", "instant": true }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [
            { "type": "value", "options": { "0": { "text": "Normal", "color": "green" } } },
            { "type": "value", "options": { "1": { "text": "Anomaly", "color": "red" } } }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 1, "color": "red" }
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "reduceOptions": { "calcs": ["max"], "values": false, "fields": "" },
        "colorMode": "value",
        "graphMode": "none",
        "textMode": "value"
      },
      "description": "Anomaly if any layer is 1 (static, z-score, seasonal, residual, multivariate). [Anomaly Center](http://localhost:3001/d/anomaly-detection-center)"
    }
  ],
  "tags": ["regional", "deep-dive", "explainability"]
}
