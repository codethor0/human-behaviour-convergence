{
  "title": "Model Performance Hub",
  "uid": "model-performance",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-30d",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "region",
        "type": "query",
        "label": "Region",
        "datasource": {
          "type": "prometheus",
          "uid": "PBFA97CFB590B2093"
        },
        "query": "label_values(behavior_index, region)",
        "multi": false,
        "includeAll": false,
        "current": {
          "selected": false,
          "text": "city_nyc",
          "value": "city_nyc"
        },
        "refresh": 1,
        "sort": 1
      },
      {
        "name": "model",
        "type": "query",
        "label": "Model",
        "datasource": {
          "type": "prometheus",
          "uid": "PBFA97CFB590B2093"
        },
        "query": "label_values(hbc_model_mae, model)",
        "multi": false,
        "includeAll": true,
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        },
        "refresh": 1,
        "sort": 1
      }
    ]
  },
  "panels": [
