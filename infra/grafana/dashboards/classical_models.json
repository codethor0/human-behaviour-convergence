{
  "title": "Classical Models Dashboard",
  "uid": "classical-models",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-30d",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "region",
        "type": "query",
        "label": "Region",
        "datasource": {
          "type": "prometheus",
          "uid": "PBFA97CFB590B2093"
        },
        "query": "label_values(behavior_index, region)",
        "multi": false,
        "includeAll": false,
        "current": {
          "selected": false,
          "text": "city_nyc",
          "value": "city_nyc"
        },
        "refresh": 1,
        "sort": 1
      },
      {
        "name": "model",
        "type": "query",
        "label": "Model",
        "datasource": {
          "type": "prometheus",
          "uid": "PBFA97CFB590B2093"
        },
        "query": "label_values(hbc_model_mae{model=~\"exponential_smoothing|arima|sarima\"}, model)",
        "multi": false,
        "includeAll": false,
        "current": {
          "selected": false,
          "text": "exponential_smoothing",
          "value": "exponential_smoothing"
        },
        "refresh": 1,
        "sort": 1
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "ETS vs ARIMA Performance - $region",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "hbc_model_mae{region=\"$region\", model=\"exponential_smoothing\"}",
          "legendFormat": "Exponential Smoothing (MAE)",
          "refId": "A"
        },
        {
          "expr": "hbc_model_mae{region=\"$region\", model=\"arima\"}",
          "legendFormat": "ARIMA (MAE)",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "MAE",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            },
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": {
              "type": "linear"
            },
            "showPoints": "never",
            "spanNulls": false
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 0.1,
                "color": "yellow"
              },
              {
                "value": 0.2,
                "color": "red"
              }
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "legend": {
          "calcs": ["last", "mean"],
          "displayMode": "table",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      }
    },
    {
      "id": 2,
      "title": "Model Metrics Summary - $region - $model",
      "type": "table",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "hbc_model_mae{region=\"$region\", model=\"$model\"}",
          "format": "table",
          "instant": true,
          "refId": "A"
        },
        {
          "expr": "hbc_model_rmse{region=\"$region\", model=\"$model\"}",
          "format": "table",
          "instant": true,
          "refId": "B"
        },
        {
          "expr": "hbc_model_mape{region=\"$region\", model=\"$model\"}",
          "format": "table",
          "instant": true,
          "refId": "C"
        },
        {
          "expr": "hbc_interval_coverage{region=\"$region\", model=\"$model\"}",
          "format": "table",
          "instant": true,
          "refId": "D"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "custom": {
            "align": "auto",
            "cellOptions": {
              "type": "auto"
            },
            "inspect": false
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 0.1,
                "color": "yellow"
              },
              {
                "value": 0.2,
                "color": "red"
              }
            ]
          },
          "unit": "short"
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Value #D"
            },
            "properties": [
              {
                "id": "unit",
                "value": "percent"
              },
              {
                "id": "min",
                "value": 0
              },
              {
                "id": "max",
                "value": 100
              }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true
      },
      "transformations": [
        {
          "id": "merge",
          "options": {}
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true,
              "instance": true,
              "job": true,
              "region": true,
              "model": true
            },
            "indexByName": {},
            "renameByName": {
              "Value #A": "MAE",
              "Value #B": "RMSE",
              "Value #C": "MAPE",
              "Value #D": "Interval Coverage %"
            }
          }
        }
      ]
    }
  ],
  "tags": ["classical", "ets", "arima", "model-comparison"]
}
