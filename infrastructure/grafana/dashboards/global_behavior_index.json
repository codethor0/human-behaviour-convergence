{
  "title": "Global Behavior Index",
  "uid": "behavior-index-global",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "templating": {
    "list": [
      {
        "name": "region",
        "type": "query",
        "label": "Region",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "query": "label_values(behavior_index, region)",
        "multi": false,
        "current": {
          "text": "city_nyc",
          "value": "city_nyc"
        },
        "refresh": 1
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "Behavior Index",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "behavior_index{region=\"$region\"}",
          "legendFormat": "Behavior Index",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            },
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": {
              "type": "linear"
            },
            "showPoints": "never",
            "spanNulls": false
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 0.6,
                "color": "yellow"
              },
              {
                "value": 0.8,
                "color": "red"
              }
            ]
          },
          "unit": "short",
          "min": 0,
          "max": 1
        }
      }
    },
    {
      "id": 2,
      "title": "Parent Sub-Indices",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "parent_subindex_value{region=\"$region\", parent=\"economic_stress\"}",
          "legendFormat": "Economic Stress",
          "refId": "A"
        },
        {
          "expr": "parent_subindex_value{region=\"$region\", parent=\"environmental_stress\"}",
          "legendFormat": "Environmental Stress",
          "refId": "B"
        },
        {
          "expr": "parent_subindex_value{region=\"$region\", parent=\"mobility_activity\"}",
          "legendFormat": "Mobility Activity",
          "refId": "C"
        },
        {
          "expr": "parent_subindex_value{region=\"$region\", parent=\"digital_attention\"}",
          "legendFormat": "Digital Attention",
          "refId": "D"
        },
        {
          "expr": "parent_subindex_value{region=\"$region\", parent=\"public_health_stress\"}",
          "legendFormat": "Public Health Stress",
          "refId": "E"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            },
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "pointSize": 5,
            "scaleDistribution": {
              "type": "linear"
            },
            "showPoints": "never",
            "spanNulls": false
          },
          "mappings": [],
          "unit": "short",
          "min": 0,
          "max": 1
        }
      }
    },
    {
      "id": 3,
      "title": "Forecast Generation Rate",
      "type": "timeseries",
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "rate(forecasts_generated_total{status=\"success\"}[5m])",
          "legendFormat": "{{region}} - Success Rate",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "Forecasts/sec",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            },
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "pointSize": 5,
            "scaleDistribution": {
              "type": "linear"
            },
            "showPoints": "never",
            "spanNulls": false
          },
          "mappings": [],
          "unit": "short"
        }
      }
    },
    {
      "id": 4,
      "title": "Forecast Computation Duration (p95)",
      "type": "timeseries",
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(forecast_computation_duration_seconds_bucket[5m])) by (le, region))",
          "legendFormat": "{{region}} - p95",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "Seconds",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            },
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "pointSize": 5,
            "scaleDistribution": {
              "type": "linear"
            },
            "showPoints": "never",
            "spanNulls": false
          },
          "mappings": [],
          "unit": "s"
        }
      }
    }
  ]
}
